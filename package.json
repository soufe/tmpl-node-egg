{
  "name": "egg",
  "TEST_PORT": "8000",
  "PROD_PORT": "9000",
  "version": "1.0.0",
  "description": "egg app boilerplate",
  "private": true,
  "egg": {
    "typescript": true,
    "declarations": true
  },
  "scripts": {
    "stop-test": "egg-scripts stop --title=egg-server-$npm_package_name-test",
    "build-test": "APP_ROOT=app/web UMI_ENV=test umi build",
    "start-test": "egg-scripts start --port=$npm_package_TEST_PORT --daemon --title=egg-server-$npm_package_name-test",
    "stop-prod": "egg-scripts stop --title=egg-server-$npm_package_name-prod",
    "build-prod": "APP_ROOT=app/web UMI_ENV=prod umi build",
    "start-prod": "egg-scripts start --port=$npm_package_PROD_PORT --daemon --title=egg-server-$npm_package_name-prod",
    "dev": "egg-bin dev",
    "umi:dev": "APP_ROOT=app/web ANALYZE=1 umi dev",
    "umi:build": "APP_ROOT=app/web ANALYZE=1 umi build",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "tsc": "ets && tsc -p tsconfig.json",
    "ci": "npm run lint && npm run cov && npm run tsc",
    "autod": "autod",
    "lint": "eslint . --ext .ts",
    "clean": "ets clean"
  },
  "dependencies": {
    "@alitajs/hd": "^2.8.2",
    "@sousoufe/jssdk": "^1.3.0",
    "@sousoufe/jsbridge": "^1.0.0",
    "@types/mocha": "^8.2.2",
    "@umijs/preset-react": "^1.8.7",
    "antd-mobile": "^2.3.4",
    "dotenv": "^9.0.2",
    "egg": "^2.29.4",
    "egg-alinode": "^2.0.1",
    "egg-bin": "^4.16.1",
    "egg-mock": "^4.1.0",
    "egg-mongoose": "^3.3.1",
    "egg-mysql": "^3.0.0",
    "egg-redis": "^2.4.0",
    "egg-scripts": "^2.13.0",
    "egg-validate": "^2.0.2",
    "egg-view-assets": "^1.7.0",
    "egg-view-nunjucks": "^2.3.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "umi": "^3.4.20"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.3",
    "@commitlint/config-conventional": "^12.1.3",
    "@commitlint/prompt": "^12.1.3",
    "@types/node": "^15.0.2",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.4",
    "@types/supertest": "^2.0.11",
    "@umijs/test": "^3.4.20",
    "autod": "^3.1.1",
    "autod-egg": "^1.1.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-dva-hmr": "^0.4.2",
    "babel-plugin-import": "^1.13.3",
    "cz-conventional-changelog": "^3.3.0",
    "egg-ci": "^1.18.0",
    "eslint": "^7.26.0",
    "eslint-config-egg": "^9.0.0",
    "eslint-plugin-react": "^7.23.2",
    "husky": "^3.1.0",
    "mockjs": "^1.1.0",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4",
    "yorkie": "^2.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "ci": {
    "version": "10"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Frank",
  "license": "MIT"
}
